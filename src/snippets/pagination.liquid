<nav aria-label="Page navigation">
  <ul class="pagination">
    {% if paginate.previous.is_link %}
    <li><a href="{{ paginate.previous.url }}" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a></li>
    {% else %}
    <li class="disabled"><a href="#" aria-label="Précédent"><span aria-hidden="true">&laquo;</span></a>
    {% endif %}
    {% for part in paginate.parts %}
        {% if part.is_link %}
        <li><a href="{{ part.url }}">{{ part.title }}</a></li>
    	{% else %}
        <li class="active"><a href="#">{{ part.title }}</a></li>
        {% endif %}
    {% endfor %}    
    {% if paginate.next.is_link %} 
    <li><a href="{{ paginate.next.url }}" aria-label="Next"><span aria-hidden="true">&raquo;</span></a></li>
    {% else %}
    <li class="disabled"><a href="#" aria-label="Suivant"><span aria-hidden="true">&raquo;</span></a>
    {% endif %} 
  </ul>
</nav>